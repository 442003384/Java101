public class readAndReplace{
public static void main(String[] args) {
     String name = "waleeeeed alshehri";
     for (int x =0;  x<=8 ; ++x){
         if(name.charAt(x) == 'e'){
        System.out.println('x');}
        else {
            System.out.println(name.charAt(x));
        }
        
     }}}
